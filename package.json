{
  "name": "starter-ts-cli",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@7.9.0",
  "description": "Starter template for TypeScript command line interface library",
  "author": "wxm",
  "license": "MIT",
  "homepage": "https://github.com/qq15725/starter-ts-cli",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qq15725/starter-ts-cli.git"
  },
  "engines": {
    "node": ">=14.18.1"
  },
  "bugs": {
    "url": "https://github.com/qq15725/starter-ts-cli/issues"
  },
  "keywords": [],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "cli": "./bin/index.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "stub": "unbuild --stub",
    "lint": "eslint src",
    "test": "vitest",
    "build": "unbuild",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "release": "bumpp package.json --commit \"release: v%s\" --push --all --tag",
    "cli": "node ./bin/index.mjs"
  },
  "devDependencies": {
    "@qq15725/eslint-config": "^1.1.0",
    "@types/node": "^18.15.3",
    "bumpp": "^9.0.0",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.36.0",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.2",
    "vitest": "^0.29.3"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "consola": "^2.15.3"
  }
}
